<?xml version="1.0" encoding="UTF-8"?>
<eventFormatter name="BasicStockQuotes" statistics="enable"
  trace="enable" xmlns="http://wso2.org/carbon/eventformatter">
  <from streamName="out" version="1.0.0"/>
  <mapping type="map">
    <property>
      <from name="price"/>
      <to name="LastTradedPrice"/>
    </property>
    <property>
      <from name="symbol"/>
      <to name="Symbol"/>
    </property>
  </mapping>
  <to eventAdaptorName="ActiveMQJMSAdaptor" eventAdaptorType="jms">
    <property name="transport.jms.Destination">BasicStockQuotes</property>
  </to>
</eventFormatter>
